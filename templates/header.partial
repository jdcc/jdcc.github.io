    <script>
      function allTags() {
        const tagNames = Array.from(
          new Set(
            Array.from(document.querySelectorAll(".tags li"))
              .map((e) => e.innerText)
              .filter((n) => n[0] != "{") // Don't grab Vue templates
          )
        ).sort();
        return tagNames;
      }
    </script>
